    "hasStringOfAttributeOfElement":      true,
    "addStringToAttributeOfElement":      true,
    "removeStringFromAttributeOfElement": true,
    "formatDate":                         true,
    "getDataURI":                         true,
    "keyCheck":                           true,
    "generateKeyString":                  true,
