{
  "strict" : true,  // "use strict" を強制
//  "indent" : 2,  // インデントの深さ
  "maxlen" : 80,  // 一行の最大長
  "curly": true, // if文などが中カッコでブロックされているか
  "undef": true, // 明示的に宣言されていない変数の使用を禁止
  "unused" : true,  // 宣言したきり使っていない変数を検出
  "eqeqeq" : true, // ==演算子と!=演算子の使用を禁止
  "eqnull" : true, // == nullに対する警告を出さない。この比較は変数がnullまたはundefinedかどうか調べる際に有用です。
  "expr" : true,  // x || (x = 1); とかができるようにする
  "trailing" : true, // 行末の空白を禁止
  "esnext": true, // ECMAScript 6

  // グローバル変数へのアクセスの管理
  "browser" : true,  // ブラウザ用のやつは許可
  "devel" : true,  //  consoleやalertを許可
  "debug"    : true,  // debugger を許可
  "globals": { // is not defined対策
    "_": false,
    "$": true,
    "jQuery": true,
    "angular": true,
    "chrome": true,
    "localStorage": true,

    "indexedDB": true,
    "IDBFactory": true,
    "IDBOpenDBRequest": true,
    "IDBRequest": true,
    "IDBDatabase": true,
    "IDBTransaction": true,
    "IDBObjectStore": true,
    "IDBKeyRange": true,
    "IDBCursor": true,
    "IDBIndex": true,

    "log": true,
    "debug": true,
    "info": true,
    "warn": true,
    "error": true,
    "dir": true,
    "trace": true,
    "assert": true,
    "dirxml": true,
    "group": true,
    "groupEnd": true,
    "time": true,
    "timeEnd": true,
    "count": true,
    "profile": true,
    "profileEnd": true,
    
    "loadTranslation": true,
    "loadSession": true,

    "formatDate": true,
    "deepCompare": true,
    "cloneObject": true,
    "getDataType": true,
    "getDataURI": true,
    "keyCheck": true,
    "generateKeyString": true,
    "toType": true,
    "unique": true,
    "trim": true,
    "arrayEqual": true,
    "dictSize": true,
    "equals": true,

    "dbCreateStores": true,
    "dbName": true,
    "dbVersion": true,
    "dbHistoryName": true,
    "dbPageInfoName": true,
    "dbDataURIName": true,
    "dbSessionName": true,
    "dbSavedSessionName": true,

    "previousSessionTimeKey": true,
    "versionKey": true,
    "defaultValues": true,
    "translationPath": true,
    "extensionExcludeUrl": true,
    "icons": true,
    "getScrollPosScript": true,
    "blankUrl": true,
    "optionPage": true,
    "changeHistory": true,
    "optionMenus": true,
    "contextMenus": true,
    "parentMenuId": true,
    "switchDisableTimerMenuItemId": true,
    "excludeDialogMenuItemId": true,

    "DISABLE_TIMER": true,
    "INVALID_EXCLUDE": true,
    "KEYBIND_EXCLUDE": true,
    "NORMAL": true,
    "TEMP_EXCLUDE": true,
    "USE_EXCLUDE": true,
    "EXTENSION_EXCLUDE": true,

    "Database": true,
    
    "deleteTick": true,
    "setTick": true,
    "unloadTimeProlong": true
  },
  "latedef": true // 定義する前に変数を利用するのを禁止にしない
}
