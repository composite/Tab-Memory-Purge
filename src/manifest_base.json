{
    "name"             : "__MSG_extName__@@extensionTitleDebug",
    "description"      : "__MSG_extDescription__",
    "version"          : "2.3.7",
    "manifest_version" : 2,
    "default_locale"   : "en",
    "incognito"        : "split",
    "icons" : {
      "48"  : "icon/icon_048.png",
      "128" : "icon/icon_128.png"
    },
    "browser_action" : {
      "default_icon"  : "icon/icon_019.png",
      "default_title" : "__MSG_extName__",
      "default_popup" : "popup.html"
    },
    "options_page" : "options.html",
    "background" : {
        "scripts" : @@backgroundScripts
    },
    "content_scripts" : [
      {
        "matches" : ["http://*/*", "https://*/*"],
        "js" : @@contentScripts
      }
    ],
    "permissions" : [
      "tabs", "storage", "history", "system.memory", "contextMenus",
      "notifications", "idle", "webRequest", "webRequestBlocking",
      "http://*/*", "https://*/*"
    ],
    "web_accessible_resources" : [
      "blank.html"
    ]
}
